{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "experimentalDecorators": true,
      "legacyDecorators": true,
      "classes": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {},
  "settings": {}
}